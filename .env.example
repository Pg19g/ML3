# EODHD API Key
# Get your API key from https://eodhd.com/
EODHD_API_KEY=your_eodhd_api_key_here

# Timezone
TZ=UTC

# Prefect Configuration for Ephemeral Mode (no server required)
# This disables the Prefect API and runs flows locally
PREFECT_API_DATABASE_CONNECTION_URL=sqlite+aiosqlite:///~/.prefect/prefect.db
PREFECT_API_URL=
PREFECT_LOGGING_LEVEL=INFO
