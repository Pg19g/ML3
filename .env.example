# EODHD API Key
# Get your API key from https://eodhd.com/
EODHD_API_KEY=your_eodhd_api_key_here

# Timezone
TZ=UTC

# Prefect Configuration
# Use 'ephemeral' for local development (no server required)
# Use 'http://localhost:4200/api' if you have Prefect server running
PREFECT_API_URL=ephemeral

# Optional: Disable Prefect telemetry
PREFECT_LOGGING_LEVEL=INFO
